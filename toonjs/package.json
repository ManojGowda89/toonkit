{
  "name": "toonkit",
  "version": "1.0.6",
  "description": "Typed Object Oriented Notation (TOON) parser and serializer for frontend and backend JavaScript applications. Lightweight, type-safe, schema-based alternative to JSON for APIs, microservices, and automation.",

  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",

  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.mjs",
      "default": "./index.cjs"
    }
  },

  "files": [
    "index.cjs",
    "index.mjs",
    "index.d.ts",
    "README.md",
    "LICENSE"
  ],

  "keywords": [
    "toon",
    "toonkit",
    "typed-notation",
    "typed-object-notation",
    "data-serialization",
    "serializer",
    "parser",
    "api-format",
    "data-format",
    "json-alternative",
    "typed-json",
    "lightweight",
    "express",
    "frontend",
    "backend",
    "microservices",
    "data-transport",
    "rest-api",
    "automation",
    "javascript",
    "nodejs",
    "nextjs",
    "react",
    "compression",
    "fast-serialization"
  ],

  "author": "Manoj Gowda B R",
  "homepage": "https://toonkit.manojgowda.in",

  "repository": {
    "type": "git",
    "url": "https://github.com/ManojGowda89/toonkit.git"
  },

  "bugs": {
    "url": "https://github.com/ManojGowda89/toonkit/issues"
  },

  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/ManojGowda89"
  },

  "license": "MIT",
  "sideEffects": false,

  "engines": {
    "node": ">=14"
  }
}